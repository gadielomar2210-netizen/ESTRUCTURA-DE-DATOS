# Programa: Serie de Fibonacci en Python

def fibonacci(n):
    a, b = 0, 1
    serie = []
    for _ in range(n):
        serie.append(a)
        a, b = b, a + b
    return serie

# Programa principal
if __name__ == "__main__":
    n = int(input("¿Cuántos términos de la serie de Fibonacci deseas ver? "))
    resultado = fibonacci(n)
    print("Serie de Fibonacci:")
    print(" ".join(map(str, resultado)))
